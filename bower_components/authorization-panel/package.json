{
  "name": "authorization-panel",
  "version": "0.5.14",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "The authorization panel used in the request panel",
  "devDependencies": {
    "polymer-cli": "^0.17.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "authorization-panel.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint authorization-panel.html",
