{
  "name": "content-type-selector",
  "version": "0.1.5",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "A content type dropdown to select request CT header",
  "devDependencies": {
    "polymer-cli": "^1.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "content-type-selector.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint content-type-selector.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"content-type-selector:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/content-type-selector.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/content-type-selector/issues",
