{
  "name": "response-history-saver",
  "version": "0.0.3",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "An element that saves requests history in a datastore",
  "devDependencies": {
    "polymer-cli": "^1.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "response-history-saver.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint response-history-saver.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"response-history-saver:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/response-history-saver.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/response-history-saver/issues",
