{
  "name": "payload-editor",
  "version": "0.2.7",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "An element that is responsible for displaying forms to prepare a request body",
  "devDependencies": {
    "bower": "^1.8.0",
    "polymer-cli": "^1.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "payload-editor.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint payload-editor.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"payload-editor:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/payload-editor.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/payload-editor/issues",
    "email": "arc@mulesoft.com"
  }
}