{
  "name": "environment-selector",
  "version": "0.1.5",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "An element to select current variables environment",
  "devDependencies": {
    "bower": "^1.8.0",
    "polymer-cli": "^1.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "environment-selector.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint environment-selector.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"environment-selector:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/environment-selector.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/environment-selector/issues",
    "email": "arc@mulesoft.com"
  }
}