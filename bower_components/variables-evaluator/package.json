{
  "name": "variables-evaluator",
  "version": "0.1.10",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "Variables evaluator for the Advanced REST Client",
  "devDependencies": {
    "bower": "^1.8.0",
    "polymer-cli": "^1.2.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "variables-evaluator.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint variables-evaluator.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"variables-evaluator:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/variables-evaluator.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/variables-evaluator/issues",
    "email": "arc@mulesoft.com"
  }
}